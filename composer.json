{
    "name": ":artemukolov/:imagedownloader",
    "type": "library",
    "description": ":image downloader",
    "keywords": [
        ":artemukolov",
        ":imagedownloader"
    ],
    "homepage": "https://github.com/artemukolov/image_downloader",
    "license": "MIT",
    "authors": [
        {
            "name": ":artemukolov",
            "email": ":artem@ukolov.info",
            "homepage": ":http://ukolov.info",
            "role": "Developer"
        }
    ],
    "require": {
        "php" : "~5.6|~7.0"
    },
    "autoload": {
        "psr-4": {
            ":vendor\\:package_name\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            ":vendor\\:package_name\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit",
        "format": "phpcbf --standard=psr2 src/"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "config": {
        "sort-packages": true
    }
}
